{
	"name": "vscode-dash",
	"displayName": "vscode-dash",
	"description": "Dash integration in Visual Studio Code",
	"version": "0.0.1",
	"publisher": "deerawan",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.dash"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.dash",
			"title": "Open in Dash"
		}],
        "keybindings": [{
            "command": "extension.dash",
            "key": "ctrl+h",
            "mac": "cmd+h",
            "when": "editorTextFocus"
        }]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}